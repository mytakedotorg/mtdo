{
  "name": "node.mytake",
  "version": "1.0.0",
  "description": "",
  "homepage": "https://github.com/mytakedotorg/mytakedotorg",
  "bugs": "https://github.com/mytakedotorg/mytakedotorg/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/mytakedotorg/mytakedotorg.git"
  },
  "main": "src/main/dist/index.js",
  "scripts": {
    "build": "gulp build",
    "formatlist": "prettier --list-different '*.js' 'src/main/scripts/**/*.ts'",
    "formatwrite": "prettier --write         '*.js' 'src/main/scripts/**/*.ts'",
    "runDev": "gulp proxy",
    "start": "node src/main/dist/server.js",
    "test": "jest",
    "test_ci": "jest --maxWorkers=2 --ci --reporters=jest-junit",
    "todd": "gulp -v"
  },
  "author": "",
  "license": "GPL-2.0",
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/jest": "^24.0.11",
    "@types/jest-image-snapshot": "^2.8.0",
    "@types/node": "^12.12.47",
    "@types/supertest": "^2.0.7",
    "axios": "^0.18.1",
    "base64-arraybuffer": "^0.1.5",
    "binary-search": "^1.3.4",
    "canvas": "^2.4.0",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "gulp": "^4.0.2",
    "gulp-nodemon": "^2.5.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^6.0.0-alpha.1",
    "http": "0.0.0",
    "jest": "^24.5.0",
    "jest-image-snapshot": "^2.8.1",
    "morgan": "^1.9.0",
    "prettier": "^1.14.0",
    "react": "^15.6.2",
    "request-promise": "^4.2.5",
    "source-map-support": "^0.5.6",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.0",
    "typescript": "3.9.5"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/src/main/scripts/.*\\.(test|spec)\\.(ts|tsx|js)$",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFiles": [],
    "verbose": true
  },
  "devDependencies": {
    "jest-junit": "^10.0.0"
  },
  "jest-junit": {
    "outputDirectory": "build/test-results/jest"
  }
}
